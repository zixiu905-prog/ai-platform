{
  "name": "aidesign-backend",
  "version": "1.0.0",
  "description": "AiDesign后端API服务",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=\"services\"",
    "test:integration": "jest --testPathPattern=\"routes\"",
    "test:e2e": "jest --testPathPattern=\"e2e\"",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "depcheck": "depcheck",
    "prisma:push:test": "DATABASE_URL=postgresql://test:test@localhost:5432/aidesign_test prisma db push --skip-generate",
    "prisma:seed:test": "DATABASE_URL=postgresql://test:test@localhost:5432/aidesign_test ts-node prisma/seed.ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "coverage:report": "node scripts/generate-coverage-report.js",
    "precommit": "npm run lint && npm run test:unit",
    "tenant:migrate": "ts-node scripts/migrate-tenant-data.ts",
    "tenant:init": "ts-node -e \"import { TenantInitializationService } from './src/services/tenantInitializationService'; const service = new TenantInitializationService(); service.initializeNewTenant({ tenantId: process.env.TENANT_ID || '', ownerUserId: process.env.OWNER_USER_ID || '', tenantName: process.env.TENANT_NAME || 'New Tenant', domain: process.env.TENANT_DOMAIN || 'new.localhost', plan: 'basic' }).then(result => console.log(result)).catch(error => console.error(error)).finally(() => service.disconnect());\""
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/jsdom": "^27.0.0",
    "@types/node-cron": "^3.0.11",
    "alipay-sdk": "^3.4.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.3.1",
    "ffmpeg-static": "^5.2.0",
    "form-data": "^4.0.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsdom": "^26.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "redis": "^4.6.11",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.1",
    "tencentcloud-sdk-nodejs": "^4.0.677",
    "uuid": "^9.0.1",
    "wechatpay-node-v3": "^2.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.7.4",
    "prisma": "^5.7.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.2"
  }
}

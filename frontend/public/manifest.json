  "shortcuts": [
    {
      "name": "新建项目",
      "short_name": "新建",
      "description": "创建新的AI项目",
      "url": "/projects/new",
      "icons": [
        {
          "src": "/icons/new-project-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "AI助手",
      "short_name": "AI助手",
      "description": "打开AI助手聊天",
      "url": "/ai-assistant",
      "icons": [
        {
          "src": "/icons/ai-assistant-96x96.png",
          "sizes": "96x96"
        }
      ]
    },
    {
      "name": "代码分析",
      "short_name": "分析",
      "description": "分析代码质量",
      "url": "/code-analysis",
      "icons": [
        {
          "src": "/icons/code-analysis-96x96.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/desktop-1.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "主界面 - 项目管理"
    },
    {
      "src": "/screenshots/desktop-2.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "AI助手界面"
    },
    {
      "src": "/screenshots/mobile-1.png",
      "sizes": "375x667",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "移动端主界面"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.aiplatform.mobile",
      "id": "com.aiplatform.mobile"
    },
    {
      "platform": "windows",
      "url": "https://microsoft.com/fwlink/applications/aiplatform"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+aiproj",
      "url": "/project?uri=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/file-handler",
      "accept": {
        "application/json": [".json"],
        "text/plain": [".txt", ".md"],
        "application/javascript": [".js", ".mjs"],
        "text/typescript": [".ts", ".tsx"],
        "text/css": [".css"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": ["image/*", "text/*", "application/*"]
        }
      ]
    }
  }
}